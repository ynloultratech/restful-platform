{
  "name": "ynloultratech/restful-platform",
  "description": "Framework designed to easily create API projects based on OpenAPI Specification (OAS)",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "YNLO-Ultratech Development Team",
      "email": "developer@ynloultratech.com",
      "homepage": "https://github.com/ynloultratech"
    }
  ],
  "keywords": [
    "api",
    "oas",
    "restful-api",
    "swagger"
  ],
  "autoload": {
    "psr-4": {
      "Ynlo\\RestfulPlatformBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ynlo\\RestfulPlatformBundle\\Demo\\": "demo/src/"
    },
    "classmap": [
      "demo/app/ApiDemoKernel.php"
    ]
  },
  "require": {
    "php": ">=7",
    "symfony/symfony": "^3.3",
    "jms/serializer-bundle": "^0.11|^1.0",
    "doctrine/annotations": "~1.0",
    "twig/twig": "~1.34|~2.4",
    "willdurand/hateoas-bundle": "^1.2",
    "knplabs/knp-paginator-bundle": "^2.5",
    "lexik/jwt-authentication-bundle": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "symfony/phpunit-bridge": "^3.1",
    "symfony/web-server-bundle": "^3.3",
    "symfony/monolog-bundle": "^3.1",
    "mockery/mockery": "dev-master",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "monolog/monolog": "^1.23",
    "doctrine/doctrine-fixtures-bundle": "^2.4"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": "dist"
  }
}
